DB::table('MiscThings')->where(Input::get('key_field_name'),'=',Input::get('key_field_value'))
->update(array(
'table_name' => Input::get('table_name'),
'record_type' => Input::get('record_type'),
'field_name' => Input::get('field_name'),
'value' => Input::get('value'),
'name' => Input::get('name')));
